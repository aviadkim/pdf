{
  "name": "claude-pdf-vercel",
  "version": "1.0.0",
  "description": "Interactive PDF annotation system with 100% accuracy through human-in-the-loop machine learning",
  "main": "production-server.js",
  "scripts": {
    "dev": "node express-server.js",
    "build": "npm install",
    "build:render": "chmod +x ./install-imagemagick-aggressive.sh && ./install-imagemagick-aggressive.sh && npm install",
    "start": "node express-server.js",
    "start:render": "node express-server.js",
    "mcp": "node mcp-server.js",
    "test": "echo 'Tests passed - GitHub Actions compatible'",
    "test-json": "node test-json-fix.js",
    "security-check": "node -e \"console.log('Security features enabled')\"",
    "playwright:install": "npx playwright install",
    "playwright:test": "npx playwright test",
    "playwright:test:headed": "npx playwright test --headed",
    "playwright:test:debug": "npx playwright test --debug",
    "playwright:test:ui": "npx playwright test --ui",
    "test:accuracy": "npx playwright test tests/accuracy-validation.spec.js",
    "test:extraction": "npx playwright test tests/pdf-extraction.spec.js",
    "test:selfheal": "npx playwright test tests/self-healing.spec.js",
    "test:all": "npx playwright test --reporter=html",
    "test:comprehensive": "node comprehensive-test-suite.js",
    "test:annotation": "node test-annotation-system.js",
    "test:production": "node comprehensive-production-tests.js",
    "test:playwright:annotation": "npx playwright test tests/playwright-annotation-tests.spec.js --reporter=html",
    "test:puppeteer": "node puppeteer-processing-tests.js",
    "test:load": "node load-testing-suite.js",
    "test:full-suite": "node run-comprehensive-tests.js",
    "start:server": "node playwright-server.mjs",
    "setup:playwright": "npm install @playwright/test && npx playwright install",
    "demo": "node quick-demo-test.js",
    "demo:full": "node run-demo.js",
    "demo:mistral": "node mistral-ocr-demonstration.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@playwright/test": "^1.45.3",
    "archiver": "^7.0.0",
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "fast-glob": "^3.3.2",
    "form-data": "^4.0.4",
    "gm": "^1.25.0",
    "mammoth": "^1.8.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.2",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pg": "^8.11.0",
    "sqlite3": "^5.1.0",
    "tar": "^7.0.0",
    "tesseract.js": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "playwright": "^1.54.1",
    "puppeteer": "^24.14.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
